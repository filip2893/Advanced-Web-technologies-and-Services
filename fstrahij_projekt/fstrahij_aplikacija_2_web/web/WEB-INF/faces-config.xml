<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
     <application>
        <locale-config>
            <default-locale>hr</default-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>org.foi.nwtis.fstrahij.i18n</base-name>
            <var>m</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{lokalizator.odaberiJezik}</from-action>
            <from-outcome>PromjenaJezika</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizator.prijava}</from-action>
            <from-outcome>prijava</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizator.registracija}</from-action>
            <from-outcome>registracija</from-outcome>
            <to-view-id>/registracija.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pog1</from-outcome>
            <to-view-id>/pogled_1.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pregledIoT</from-outcome>
            <to-view-id>/pregledIoT.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pog3</from-outcome>
            <to-view-id>/pogled_3.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pog4</from-outcome>
            <to-view-id>/pogled_4.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dnevnikRada</from-outcome>
            <to-view-id>/dnevnikRada.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mail</from-outcome>
            <to-view-id>/mail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/prijava.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registracija</from-outcome>
            <to-view-id>/registracija.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/registracija.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>prijava</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pogled_1.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pregledIoT.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pogled_3.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pogled_4.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mail.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
